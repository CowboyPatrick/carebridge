<%= simple_form_for(@order) do |f| %>
  <%= hidden_field_tag :button_id, @button.id %>
  <div class="form-group">
    <label class="control-label check_boxes"></label>
    <!-- <input type="hidden" name="order[hidden]"> -->
    <div class="facetime-choices">
      <% @button.provider_actions.where(user: current_user).each_with_index do |provider_action, index| %>
        <label for="post_category_ids_<%= provider_action.id %>" class="facetime-choice">
          <%= cl_image_tag (provider_action.photo.attached? ? provider_action.photo.key : "https://images.unsplash.com/photo-1519340241574-2cec6aef0c01?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=2089&q=80" ), alt: "alttext" %>
          <h1><%= provider_action.name %></h1>
        </label>
      <% end %>
    </div>
  </div>
<% end %>