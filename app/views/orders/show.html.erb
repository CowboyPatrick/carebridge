<div class="container pt-5">
  <h1 style=" margin-top: 1em ">Orders</h1>
  <div class= "menu-btn"> <%=link_to buttons_path, class:'btn btn-return' do %>
      Return
    <% end %>
  </div>
  <div class="container p-5">
    <table class="table table-striped table-warning table-hover">
      <thead>
        <tr>
          <th scope="col">Item:</th>
          <th scope="col">Time:</th>
        </tr>
      </thead>
      <% @order.provider_actions.each do |provider_action|%>
        <tr>
          <td> <%= provider_action.name%></td>
          <td><%= @order.created_at.in_time_zone("Asia/Tokyo").strftime("%b %e, %l:%M %p")  %></td>
        </tr>
      <%end %>
    </table>
  </div>
</div>